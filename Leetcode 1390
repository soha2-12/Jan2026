class Solution:
    def sumFourDivisors(self, nums):
        total = 0

        for n in nums:
            divs = set()
            i = 1
            while i * i <= n:
                if n % i == 0:
                    divs.add(i)
                    divs.add(n // i)
                i += 1

            if len(divs) == 4:
                total += sum(divs)

        return total
